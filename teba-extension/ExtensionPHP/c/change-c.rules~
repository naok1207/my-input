{ $:LANG }
=> { 'w*':LANG 'で書かれていました':MES '\n':SP_NL }

# IF_model
{ $:MODEL/IF/ $:B_COND $cond:ANY $:E_COND }
=> { 'if':CT_IF '(':P_R $cond:COND ')':P_L '{':C_L '\n':SP_NL }

# ELSEIF_model
{ $:MODEL/ELSEIF/ $:B_COND $cond:ANY $:E_COND }
=> { 'else':CT_EL ' ':SP_B 'if':CT_IF '(':P_R $cond:COND ')':P_R '{':C_L '\n':SP_NL }

# ELSE_model
{ $:MODEL/ELSE/ }
=> { 'else':CT_EL '{':C_L '\n':SP_NL }

# WHILE_model
{ $:MODEL/WHILE/ $:B_COND $cond:ANY $:E_COND }
=> { 'while':CT_BE '(':P_R $cond:COND ')':P_L '{':C_L '\n':SP_NL }

# FOR_model(end_NOT_include)
{ $:MODEL/FOR/ $var:VAR $:B_INI $ini:ANY $:E_INI $:B_END $end:ANY $:E_END $:END_INCLUDE/False/ }
  => { 'for':CT_BE '(':P_L $var:ID_C '=':OP $ini:INI ';':SC $var:ID_C '<':OP $end:END ';':SC $var:ID_C '++':OP ')':P_R '{':C_L '\n':SP_NL}

# FOR_model(end_include)
{ $:MODEL/FOR/ $var:VAR $:B_INI $ini:ANY $:E_INI $:B_END $end:ANY $:E_END $:END_INCLUDE/True/ }
   => { 'for':CT_BE '(':P_L $var:ID_C '=':OP $ini:INI ';':SC $var:ID_C '<=':OP $end:END ';':SC $var:ID_C '++':OP ')':P_R '{':C_L '\n':SP_NL}

# FOREACH_model
{ $:MODEL/FOREACH/ $var:VAR $array:ARRAY }
 => { 'for':CT_BE '(':P_L 'i':ARRAY_IDX '=':OP '0':LIN ';':SC 'i':ARRAY_IDX '<':OP '/*array_size*/':ARRAY_SIZE ';':SC 'i':ID_C '++':OP ')':P_R '{':C_L '\n':SP_NL $var:ID_C '=':OP $array:ARRAY '[i]':ARRAY_IDX '\n':SP_NL}
