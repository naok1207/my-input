# IF_model
{  $:MODEL/IF/ $:B_COND $cond:ANY $:E_COND }
=> { 'if':CT_IF ' ':SP_B $cond:COND ':':CO }

# ELSEIF_model
{  $:MODEL/ELSEIF/ $:B_COND $cond:ANY $:E_COND }
=> { 'elif':CT_EIF ' ':SP_B $cond:COND ':':CO }

# ELSE_model
{  $:MODEL/ELSE/ }
=> { 'else':CT_EL ':':CO }

# WHILE_model
{  $:MODEL/WHILE/ $:B_COND $cond:ANY $:E_COND }
=> { 'while':CT_BE ' ':SP_B $cond:COND ':':CO }

# FOR_model(end_NOT_include)
{  $:MODEL/FOR/ $var:VAR $:B_INI $ini:ANY $:E_INI $:B_END $end:ANY $:E_END $:END_INCLUDE/False/ }
  => {'for':CT_BE ' ':SP_B $var:ID_C ' ':SP_B 'in':IN ' ':SP_B 'range':ID_C '(':P_L $ini:INI ',':CA $end:END ')':P_R ':':CO }

# FOR_model(end_include)
{  $:MODEL/FOR/ $var:VAR $:B_INI $ini:ANY $:E_INI $:B_END $end:ANY $:E_END $:END_INCLUDE/True/ }
  => {'for':CT_BE ' ':SP_B $var:ID_C ' ':SP_B 'in':IN ' ':SP_B 'range':ID_C '(':P_L $ini:INI ',':CA $end:END '+':OP '1':LIN ')':P_R ':':CO }

# FOREACH_model
{  $:MODEL/FOREACH/ $var:VAR $:B_ARR $array:ANY $:E_ARR }
=> { 'for':CT_BE ' ':SP_B $var:ID_C ' ':SP_B 'in':IN ' ':SP_B $array:ARRAY ':':CO }
