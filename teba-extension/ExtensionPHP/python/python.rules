# if 
{ $:CT_IF $:SP
  $[cond: $[: $:B_P $:ANY $:E_P $]+ $] $:SP
  $:CO}
  => { 'Python':LANG 'IF':MODEL '':B_COND $cond '':E_COND}
  
{ $:CT_IF $:SP
  $[: $:P_L $:SP $]
  $[cond: $[: $:B_P $:ANY $:E_P $]+ $]
