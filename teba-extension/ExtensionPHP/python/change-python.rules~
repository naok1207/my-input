# IF_model
{ $:LANG $:MODEL/IF/ $:B_COND $cond:ANY $:E_COND }
=> { 'if':CT_IF ' ':SP_B $cond:COND ':':CO '\n':SP_NL }

# ELSEIF_model
{ $:LANG $:MODEL/ELSEIF/ $:B_COND $cond:ANY $:E_COND }
=> { 'elif':CT_EIF ' ':SP_B $cond:COND ':':CO '\n':SP_NL }

# ELSE_model
{ $:LANG $:MODEL/ELSE/ }
=> { 'else':CT_EL ':':CO '\n':SP_NL }

# WHILE_model
{ $:LANG $:MODEL/WHILE/ $:B_COND $cond:ANY $:E_COND }
=> { 'while':CT_BE ' ':SP_B $cond:COND ':':CO '\n':SP_NL }

# FOR_model(end_NOT_include)
{ $:LANG $:MODEL/FOR/ $var:VAR $:B_INI $ini:ANY $:E_INI $:B_END $end:ANY $:E_END $:END_INCLUDE/False/ }
  => {'for':CT_BE ' ':SP_B $var:ID_C ' ':SP 'in':IN ' ':SP_B 'range':ID_C '(':P_L $ini:INI ',':CA $end:END ')':P_R ':':CO '\n':SP_NL }

# FOR_model(end_include)
{ $:LANG $:MODEL/FOR/ $var:VAR $:B_INI $ini:ANY $:E_INI $:B_END $end:ANY $:E_END $:END_INCLUDE/True/ }
  => {'for':CT_BE ' ':SP_B $var:ID_C ' ':SP 'in':IN ' ':SP_B 'range':ID_C '(':P_L $ini:INI ',':CA $end+1:END ')':P_R ':':CO '\n':SP_NL }

# FOREACH_model
{ $:LANG $:MODEL/FOREACH/ $var:VAR $array:ARRAY }
=> { 'for':CT_BE ' ':SP_B $var:ID_C ' ':SP_B 'in':IN ' ':SP_B $array:ARRAY ':':CO '\n':SP_NL }
