# IF_model
{  $:MODEL/IF/ $:B_COND $cond:ANY $:E_COND }
=> { 'if':CT_IF ' ':SP_B $cond:COND ' ':SP_B 'then':THEN }

# ELSEIF_model
{  $:MODEL/ELSEIF/ $:B_COND $cond:ANY $:E_COND }
=> { 'elsif':CT_EIF ' ':SP_B $cond:COND ' ':SP_B 'then':THEN }

# ELSE_model
{  $:MODEL/ELSE/ }
=> { 'else':CT_EL }

# WHILE_model
{  $:MODEL/WHILE/ $:B_COND $cond:ANY $:E_COND }
=> { 'while':CT_BE ' ':SP_B $cond:COND ' ':SP_B 'do':CT_DO }

# FOR_model(end_NOT_include)
{  $:MODEL/FOR/ $var:VAR $:B_INI $ini:ANY $:E_INI $:B_END $end:ANY $:E_END $:END_INCLUDE/False/ }
=> {'for':CT_BE ' ':SP_B $var:ID_C ' ':SP 'in':IN ' ':SP_B $ini '...':OP_Y $end ' ':SP_B 'do':CT_DO }

# FOR_model(end_include)
{  $:MODEL/FOR/ $var:VAR $:B_INI $ini:ANY $:E_INI $:B_END $end:ANY $:E_END $:END_INCLUDE/True/ }
=> {'for':CT_BE ' ':SP_B $var:ID_C ' ':SP 'in':IN ' ':SP_B $ini:INI '..':OP_X $end:END ' ':SP_B 'do':CT_DO }

# FOREACH_model
{ $:MODEL/FOREACH/ $var:VAR $:B_ARR $array:ANY $:E_ARR}
=> { 'for':CT_BE ' ':SP_B $var:ID_C ' ':SP_B 'in':IN ' ':SP_B $array:ID_C ' ':SP_B 'do':CT_DO }


#expr translate
{ $:OP/===/ } => { '==':OP }
{ $:OP/!==/ } => { '!=':OP }
{ $:OP/and/ } => { '&&':OP }
{ $:OP/or/  } => { '\|\|':OP }
{ $x:ID_C/\$/ $## $y:ID_C/\w+/ } => { $y:ID_C }
{ $x:ID_V/\$/ $## $y:ID_V/\w+/ } => { $y:ID_C }
