# foreach
{ $:CT_BE/foreach/ $:SP $:P_L $:SP
  $#2:B_P
  $#3:B_P $array:ID_V $#3:E_P $:SP
  $:OP/as/ $:SP
  $#4:B_P $var:ID_V $#4:E_P $:SP
  $#2:E_P $:P_R $:SP
  $[: $:C_L $]?
  }
  => {'PHP':LANG 'FOREACH':MODEL $var:VAR $array:ARRAY}
